<com.cloudbees.jenkins.plugins.sshcredentials.impl.BasicSSHUserPrivateKey plugin="<%= @crdPlugin %>">
    <scope>GLOBAL</scope>
    <id><%= @crdId %></id>
    <description></description>
    <username><%= @crdUsername %></username>
    <passphrase><%= @crdPassphrase %></passphrase>
    <privateKeySource class="com.cloudbees.jenkins.plugins.sshcredentials.impl.BasicSSHUserPrivateKey$DirectEntryPrivateKeySource">
        <privateKey><%= @crdPrivateKey %></privateKey> 
    </privateKeySource> 
</com.cloudbees.jenkins.plugins.sshcredentials.impl.BasicSSHUserPrivateKey>